{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "CreateRoute",
  "title": "Update the administration Routing module file",
  "properties": {
    "name": {
      "type": "string",
      "description": "append the given name to AdministrationRoutingModule file",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "Route name in AppRoutes constant",
      "default": "JOB_TITLE"
    }
  },
  "required": [
    "name"
  ]
}

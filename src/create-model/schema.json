{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "CreateModel",
  "title": "Create for given name",
  "properties": {
    "name": {
      "type": "string",
      "description": "Name of your model that you want to create",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "Name of your model that you want to create?",
      "default": "JobTitle"
    }
  },
  "required": [
    "name"
  ]
}

{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "GenerateNewService",
  "title": "create scaffold for e-service",
  "properties": {
    "name": {
      "type": "string",
      "description": "menu name",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "default": "JOB_TITLE",
      "x-prompt": "Please enter the endpoint name"
    },
    "parent": {
      "type": "string",
      "description": "Parent name",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "default": null,
      "x-prompt": "Please write the Parent Name like (ADMINISTRATION)"
    }
  },
  "required": [
    "name",
    "parent"
  ]
}

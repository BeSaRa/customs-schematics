{
  "name": "customs-schematics",
  "version": "0.0.0",
  "description": "A blank schematics",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "check:create-endpoint": "npm run build && schematics .:create-endpoint",
    "apply:create-endpoint": "npm run build && schematics .:create-endpoint --debug=false",
    "check:create-app-full-route": "npm run build && schematics .:create-app-full-route",
    "apply:create-app-full-route": "npm run build && schematics .:create-app-full-route --debug=false",
    "check:create-app-route": "npm run build && schematics .:create-app-route",
    "apply:create-app-route": "npm run build && schematics .:create-app-route --debug=false",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "schematics"
  ],
  "author": "BeSaRa",
  "ng-add": {
    "save": "devDependencies"
  },
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^15.2.7",
    "@angular-devkit/schematics": "^15.2.7",
    "typescript": "~4.9.3"
  },
  "devDependencies": {
    "@schematics/angular": "^15.2.7",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^14.15.0",
    "jasmine": "^4.0.0"
  },
  "repository": {
    "url": "https://github.com/BeSaRa/customs-schematics.git"
  }
}
